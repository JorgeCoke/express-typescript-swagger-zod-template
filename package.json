{
  "name": "express-typescript-swagger-zod-template",
  "version": "1.0.0",
  "description": "Express Typescript Swagger and Zod template",
  "engines": {
    "node": ">=21.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JorgeCoke/express-typescript-swagger-zod-template.git"
  },
  "author": "JorgeCoke",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JorgeCoke/express-typescript-swagger-zod-template/issues"
  },
  "homepage": "https://github.com/JorgeCoke/express-typescript-swagger-zod-template#readme",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "npx rimraf dist && tsc",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.{ts,js}' && tsc --noEmit",
    "format": "prettier --write 'src/**/*.{ts,js}'",
    "format:staged": "lint-staged",
    "npm:audit": "better-npm-audit audit",
    "npm:check": "npm-check -u",
    "release": "branch=$(git branch --show-current); if [[ $branch == 'master' || $branch == 'main' ]]; then commit-and-tag-version; else echo 'Releases allowed from main branch only'; exit 1; fi;",
    "cz": "cz",
    "devmoji": "devmoji -e --lint",
    "prepare": "husky"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.0.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.0.2",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.2.2",
    "rotating-file-stream": "^3.2.1",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "better-npm-audit": "^3.7.3",
    "commit-and-tag-version": "^12.3.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "devmoji": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-sonarjs": "^0.25.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-check": "^6.0.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  }
}
